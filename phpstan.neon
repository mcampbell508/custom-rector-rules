includes:
    - vendor/symplify/phpstan-rules/config/symplify-rules.neon
    - phpstan-baseline.php

parameters:
    level: 8

    # requires exact closure types
    checkMissingCallableSignature: true

    paths:
        - rector.php
        - config
        - src
        - tests

    checkGenericClassInNonGenericObjectType: false